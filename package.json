{
  "name": "esbuild-css-as-string",
  "version": "1.0.8",
  "description": "Transform your CSS files into string",
  "type": "module",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svgor-code/esbuild-css-as-string.git"
  },
  "scripts": {
    "build": "esbuild src/index.js --outfile=dist/index.js",
    "types": "tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir types"
  },
  "author": "Konstantin Troitskiy",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.20.1"
  },
  "keywords": [
    "esbuild-plugin",
    "css-as-string",
    "css-import",
    "shadow-dom",
    "web-components",
    "style-encapsulation",
    "frontend",
    "javascript",
    "css",
    "esbuild-css"
  ]
}
